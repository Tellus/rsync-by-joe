    "@actions/io": "^1.1.1",
    "chance": "^1.1.7"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/chance": "^1.1.2",
    "@types/jest": "^26.0.23",
    "@vercel/ncc": "^0.28.6",
